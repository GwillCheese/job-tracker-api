generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
}

model Application {
  id          Int      @id @default(autoincrement())
  companyName String
  jobTitle    String
  status      String
  createdAt   DateTime @default(now())
}
